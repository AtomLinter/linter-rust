{
  "name": "linter-rust",
  "main": "./lib/init",
  "version": "1.0.1",
  "private": true,
  "description": "Lint Rust-files, using rustc and/or cargo",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-rust.git"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=1.33.0 <2.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "2.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "10.0.0",
    "atom-package-deps": "5.1.0",
    "semver": "7.1.3",
    "xregexp": "4.4.1"
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "semanticCommits": true,
    "rangeStrategy": "pin",
    "packageRules": [
      {
        "packagePatterns": [
          "^eslint"
        ],
        "groupName": "ESLint packages"
      }
    ]
  },
  "package-deps": [
    "linter:2.0.0"
  ]
}
